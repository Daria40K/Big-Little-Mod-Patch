<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded - More Plants</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<!-- ========== Vanilla Cooking Expanded ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Vanilla Cooking Expanded</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
				<!-- ========== GloomyFurniture (Continued) ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>GloomyFurniture (Continued)</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- VCE_VegMilkExtractor -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="VCE_VegMilkExtractor"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>GL_Cupboard</li>
									<li>GL_TablewareShelf</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== More Archotech Garbage (Continued) ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>More Archotech Garbage (Continued)</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- VCE_VegMilkExtractor -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="VCE_VegMilkExtractor"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>ArchoCookingAssistant</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== More Linkables ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>More Linkables</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- VCE_VegMilkExtractor -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="VCE_VegMilkExtractor"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>KitchenCupboard</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== VGP Garden Gourmet ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>VGP Garden Gourmet</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- VCE_VegMilkExtractor -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="VCE_VegMilkExtractor"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>CookingTools</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== Vanilla Furniture Expanded - Production ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Vanilla Furniture Expanded - Production</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- VCE_VegMilkExtractor -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="VCE_VegMilkExtractor"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>VFE_KitchenSinkCabinet</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== WorkBench Support - More Linkable Facilities - ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>WorkBench Support - More Linkable Facilities -</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- VCE_VegMilkExtractor -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="VCE_VegMilkExtractor"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>Link_KitchenGoods</li>
									<li>Link_KitchenBoost</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== [LTS]Furnishing ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>[LTS]Furnishing</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- VCE_VegMilkExtractor -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="VCE_VegMilkExtractor"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>LTS_Sink</li>
									<li>LTS_Fridge</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
						</operations>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>