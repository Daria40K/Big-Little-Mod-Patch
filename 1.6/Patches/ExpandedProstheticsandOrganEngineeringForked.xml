<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Expanded Prosthetics and Organ Engineering - Forked</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<!-- ========== Core ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Core</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- TableSimpleProsthetic | TableBionics | TableBionics | TableOrgans | TableOrgans -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="TableSimpleProsthetic" or defName="TableBionics" or defName="TableBionics" or defName="TableOrgans" or defName="TableOrgans"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>ToolCabinet</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== RimLife Expansion Production Plus ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>RimLife Expansion Production Plus</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- TableSimpleProsthetic | TableBionics | TableBionics | TableOrgans | TableOrgans -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="TableSimpleProsthetic" or defName="TableBionics" or defName="TableBionics" or defName="TableOrgans" or defName="TableOrgans"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>RLE_PowerStabilizer</li>
									<li>RLE_IntegratedManagementDevice</li>
									<li>RLE_CMTCleaner</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>