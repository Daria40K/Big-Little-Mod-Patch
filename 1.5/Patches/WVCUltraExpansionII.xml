<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>WVC - Ultra Expansion II</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<!-- WVC_Ultra_GarbageRecyclerBench -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class ="CompProperties_AffectedByFacilities"]</xpath>
					<nomatch Class="PatchOperationConditional">
						<xpath>Defs/ThingDef[defName="WVC_Ultra_GarbageRecyclerBench"]/comps</xpath>
						<match Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="WVC_Ultra_GarbageRecyclerBench"]/comps</xpath>
							<value>
								<li Class="CompProperties_AffectedByFacilities">
									<linkableFacilities />
								</li>
							</value>
						</match>
						<nomatch Class="PatchOperationAdd">
							<xpath>Defs/ThingDef[defName="WVC_Ultra_GarbageRecyclerBench"]</xpath>
							<value>
								<comps>
									<li Class="CompProperties_AffectedByFacilities">
										<linkableFacilities/>
									</li>
								</comps>
							</value>
						</nomatch>
					</nomatch>
				</li>
				<!-- ========== Core ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Core</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants | WVC_Ultra_GarbageRecyclerBench -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants" or defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>ToolCabinet</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== Akeron - Decorations ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Akeron - Decorations</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants | WVC_Ultra_GarbageRecyclerBench -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants" or defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>AKN_MotivatingPlush</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== Cybernetic Storm ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Cybernetic Storm</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants | WVC_Ultra_GarbageRecyclerBench -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants" or defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>CS_ReAnalyser</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== Even More Linkables ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Even More Linkables</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>HobbesLink_Still</li>
									<li>HobbesLink_Rotovape</li>
									<li>HobbesLink_TabletopCentrifuge</li>
									<li>HobbesLink_TabletopStove</li>
									<li>HobbesLink_TabletopLabTubeRack</li>
									<li>HobbesLink_CNCMachine</li>
									<li>HobbesLink_3DPrinter</li>
									<li>HobbesLink_RobotAssistant</li>
									<li>HobbesLink_Lathe</li>
									<li>HobbesLink_Spacer_Lathe</li>
									<li>HobbesLink_Vice</li>
									<li>HobbesLink_Centrifuge</li>
									<li>HobbesLink_IndustrialOven</li>
									<li>HobbesLink_ConveyorBelt</li>
									<li>HobbesLink_Incinerator</li>
								</value>
							</li>
							<!-- WVC_Ultra_GarbageRecyclerBench -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>HobbesLink_CNCMachine</li>
									<li>HobbesLink_3DPrinter</li>
									<li>HobbesLink_RobotAssistant</li>
									<li>HobbesLink_Lathe</li>
									<li>HobbesLink_Spacer_Lathe</li>
									<li>HobbesLink_Vice</li>
									<li>HobbesLink_LaserEngraver</li>
									<li>HobbesLink_IndustrialOven</li>
									<li>HobbesLink_ConveyorBelt</li>
									<li>HobbesLink_Incinerator</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== Expanded Prosthetics and Organ Engineering - Forked ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Expanded Prosthetics and Organ Engineering - Forked</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>TableBionics</li>
									<li>TableOrgans</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== More Linkables ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>More Linkables</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants | WVC_Ultra_GarbageRecyclerBench -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants" or defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>ToolCabinetA</li>
									<li>ToolCabinetB</li>
									<li>ToolCabinetC</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== RimLife Expansion Production Plus ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>RimLife Expansion Production Plus</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>RLE_PowerStabilizer</li>
									<li>RLE_IntegratedManagementDevice</li>
									<li>RLE_CMTCleaner</li>
								</value>
							</li>
							<!-- WVC_Ultra_GarbageRecyclerBench -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>RLE_PowerStabilizer</li>
									<li>RLE_IntegratedManagementDevice</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== UN-Colony (Continued) ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>UN-Colony (Continued)</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants | WVC_Ultra_GarbageRecyclerBench -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants" or defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>SadjuukShaper</li>
									<li>SadjuukGrinding</li>
									<li>SadjuukWeldingRobot</li>
									<li>SadjuukLathe</li>
									<li>SadjuukMilling</li>
									<li>SadjuukAssemblyRobot</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<!-- ========== WorkBench Support - More Linkable Facilities - ========== -->
				<li Class="PatchOperationFindMod">
					<mods>
						<li>WorkBench Support - More Linkable Facilities -</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<!-- WVC_Ultra_WorkBenches_Implants | WVC_Ultra_GarbageRecyclerBench -->
							<li Class="PatchOperationAdd">
								<xpath>Defs/ThingDef[@Name="WVC_Ultra_WorkBenches_Implants" or defName="WVC_Ultra_GarbageRecyclerBench"]/comps/li[@Class="CompProperties_AffectedByFacilities"]/linkableFacilities</xpath>
								<value>
									<li>Link_UniversalCabinet</li>
								</value>
							</li>
						</operations>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>